{
  "plugins": [
    "stylelint-scss",
		"stylelint-order"
	],
  "extends": [
    "stylelint-config-recommended",
    "stylelint-config-styled-components"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "order/properties-alphabetical-order": true,
    "color-no-invalid-hex": null
  }
}